[ignore]
.*/node_modules/fbjs/.*
.*/node_modules/config-chain/test/broken.json
.*/node_modules/npmconf/test/.*

[include]

[libs]

[lints]
# all=off by default
all=warn
untyped-type-import=error
sketchy-null-bool=off

[options]

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
